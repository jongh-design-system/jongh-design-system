{
  "name": "@jongh/ui",
  "version": "1.5.21",
  "description": "",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./styles.css": "./dist/styles.css",
    ".": {
      "types": "./dist/src/components/index.d.ts",
      "require": "./dist/index.umd.js",
      "import": "./dist/index.es.js"
    }
  },
