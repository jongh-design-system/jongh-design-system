    "dist/",
    "package.json"
  ],
  "scripts": {
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "local": "vite",
    "type": "tsc -p tsconfig.json --skipLibCheck && tsc-alias -p tsconfig.json",
    "storybook": "storybook dev -p 6006",
    "build": "pnpm prepare && vite build",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "dev": "pnpm panda --watch",
    "codegen": "pnpm panda codegen --clean",
    "cssgen": "pnpm panda cssgen",
    "release": "pnpm panda prepare && vite build && pnpm panda cssgen && pnpm build-storybook",
    "prepare": "pnpm panda codegen --clean",
    "check-types": "tsc --noEmit",
    "studio": "pnpm panda studio",
    "pub": "pnpm publish --no-git-checks"
  },
  "dependencies": {
    "@fontsource-variable/roboto-condensed": "^5.0.3",
