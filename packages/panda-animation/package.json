{
  "name": "panda-animation",
  "version": "0.0.1",
  "description": "animation preset for pandacss",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "main": "./src/index.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "clean-package": {
    "replace": {
      "exports": {
        ".": {
          "import": {
            "types": "./dist/preset.d.ts",
            "default": "./dist/preset.js"
          },
          "require": {
            "types": "./dist/preset.d.cts",
            "default": "./dist/preset.cjs"
          }
        }
      }
    }
  },
  "scripts": {
    "release": "pnpm build && pnpm publish --no-git-checks",
    "prepack": "clean-package",
    "postpack": "clean-package restore",
    "check-type": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "build": "tsup"
  },
  "tsup": {
    "entry": [
      "src/preset.ts"
    ],
    "clean": true,
    "dts": true,
    "sourcemap": true,
    "format": [
      "esm",
      "cjs"
    ]
  },
  "devDependencies": {
    "clean-package": "^2.2.0",
    "tsup": "^8.3.0"
  }
}
