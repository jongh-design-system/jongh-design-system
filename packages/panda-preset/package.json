{
  "name": "@jongh/panda-preset",
  "version": "1.3.0",
  "description": "pandacss preset",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "main": "./src/preset.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src",
    "package.json"
  ],
  "clean-package": {
    "replace": {
      "exports": {
        ".": {
          "import": {
            "types": "./dist/preset.d.ts",
            "default": "./dist/preset.js"
          },
          "require": {
            "types": "./dist/preset.d.cts",
            "default": "./dist/preset.cjs"
          }
        }
      }
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsup",
    "release": "pnpm build && pnpm publish --no-git-checks",
    "prepack": "clean-package",
    "postpack": "clean-package restore",
    "prepare": "pnpm build"
  },
  "tsup": {
    "entry": [
      "src/preset.ts"
    ],
    "clean": true,
    "dts": true,
    "sourcemap": true,
    "format": [
      "esm",
      "cjs"
    ]
  },
  "devDependencies": {
    "@pandacss/dev": "^0.40.1",
    "clean-package": "^2.2.0",
    "tsup": "^8.3.0"
  }
}
