{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": ["dist/**", ".next/**", "out/**"],
      "dependsOn": ["^build"]
    },
    "docs#build": {
      "dependsOn": ["@jongh/cli#build"]
    },
    "check-type": {},
    "lint": {},
    "test": {
      "dependsOn": ["check-type", "lint"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
